{
  "name": "bitjoe-server-js",
  "version": "0.0.1",
  "description": "tradle bitjoe component JS web server",
  "main": "index.js",
  "bin": {
    "bitjoe-server": "./app.js"
  },
  "scripts": {
    "jsbeautify": "js-beautify -r {*.js,**/*.js} --config=node_modules/tradle-style/.jsbeautifyrc",
    "jshint": "jshint {*.js,**/*.js} --exclude-path=.gitignore --config=node_modules/tradle-style/.jshintrc",
    "jscs": "jscs {*.js,**/*.js} --config=node_modules/tradle-style/.jscsrc"
  },
  "pre-commit": [
    "jscs",
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bitjoe-server-js.git"
  },
  "dependencies": {
    "body-parser": "^1.10.1",
    "concat-stream": "^1.4.7",
    "debug": "^2.1.1",
    "express": "^4.10.7",
    "tradle-utils": "git://github.com/tradle/tradle-utils.git",
    "bitjoe-js": "git://github.com/tradle/bitjoe-js.git",
    "minimist": "^1.1.0"
  },
  "keywords": [
    "bitcoin",
    "blockchain",
    "tradle",
    "blockchain",
    "dapp"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tradle/bitjoe-server-js/issues"
  },
  "homepage": "https://github.com/tradle/bitjoe-server-js",
  "devDependencies": {
    "js-beautify": "latest",
    "jshint": "latest",
    "pre-commit": "0.0.11",
    "tape": "^3.0.3",
    "tradle-style": "latest"
  }
}
